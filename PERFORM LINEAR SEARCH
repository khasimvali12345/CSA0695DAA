#include<stdio.h>
int main()
{
 int a[20],i,x,n;
 printf("Enter number of elements: ");
 scanf("%d",&n);
 printf("Enter the elements: ");
 for(i=0;i<n;i++)
 {
  scanf("%d",&a[i]);
 }
 printf("enter the key element: ");
 scanf("%d",&x);
 for(i=0;i<n;i++)
 {
   if(a[i]==x)
   break;
 }
 if(i<n)
  printf("Element found in position %d",i);
 else
  printf("Element not found");
 return 0;
}
